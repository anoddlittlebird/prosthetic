{
	"manifest_version": 3,
	"name": "Prosthetic",
	"version": "1.2",
	"description": "Attentional Prosethic (M. V. 3)",
	"content_scripts":[{
			"js": ["patterns.js","content_script.js"],
			"matches": ["*://*/*"],
			"css" : ["ext_styles.css"],
			"run_at": "document_end"
		}],

	"incognito": "not_allowed",
	"icons":{
		"128":"icon128.png"
	},
	"permissions":[
		"storage"
	],
	"web_accessible_resources": [
		{
	"resources":["*.png"],
	"matches": ["*://*/*"]
		}
	]
}
